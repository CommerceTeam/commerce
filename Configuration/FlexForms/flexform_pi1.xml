<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<!--  <langChildren>1</langChildren>!-->
	</meta>
	<sheets>
		<s_product>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.sheet_product</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<type>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.type</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.type.normal</numIndex>
										<numIndex index="1">normal</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.type.teaser</numIndex>
										<numIndex index="1">teaser</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</type>
					<numberOfTopproducts>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.numberOfTopproducts</label>
							<config>
								<type>input</type>
								<size>2</size>
							</config>
						</TCEforms>
					</numberOfTopproducts>
					<product_id>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.product_name</label>
							<config>
								<type>select</type>
								<itemsProcFunc>CommerceTeam\Commerce\Utility\TceformsUtility->productsSelector</itemsProcFunc>
								<foreign_table>tx_commerce_products</foreign_table>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</product_id>
					<StartCategory>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.category_selection</label>
							<config>
								<type>select</type>
								<foreign_table>tx_commerce_categories</foreign_table>
								<foreign_table_where> AND sys_language_uid = 0</foreign_table_where>
								<size>10</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<!-- <renderMode>tree</renderMode> -->
								<treeConfig>
									<expandAll>1</expandAll>
									<parentField>parent_category</parentField>
									<appearance>
										<showHeader>1</showHeader>
									</appearance>
								</treeConfig>
							</config>
						</TCEforms>
					</StartCategory>
				</el>
			</ROOT>
		</s_product>
		<s_template>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.sheet_template</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<template>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.template</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>html,htm,tmpl,tpl,txt</allowed>
								<uploadfolder>uploads/tx_commerce</uploadfolder>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</template>
					<listPid>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.listPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</listPid>
					<singlePid>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.singlePid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</singlePid>
					<displayPID>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.displayPID</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</displayPID>
					<basketPID>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.basketPID</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</basketPID>
					<maxRecords>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.maxRecords</label>
							<config>
								<type>input</type>
								<size>2</size>
							</config>
						</TCEforms>
					</maxRecords>
					<maxPages>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.maxPages</label>
							<config>
								<type>input</type>
								<size>2</size>
							</config>
						</TCEforms>
					</maxPages>
					<showPageBrowser>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.showPageBrowser</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</showPageBrowser>
					<dontLinkActivePage>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.dontLinkActivePage</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</dontLinkActivePage>
					<showFirstLast>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.showFirstLast</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</showFirstLast>
					<showRange>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.showRange</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</showRange>
					<showItemCount>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.showItemCount</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</showItemCount>
					<hscText>
						<TCEforms>
							<label>LLL:EXT:commerce/Resources/Private/Language/locallang_be.xml:tx_commerce_pi1.pi_flexform.hscText</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</hscText>
				</el>
			</ROOT>
		</s_template>
	</sheets>
</T3DataStructure>
